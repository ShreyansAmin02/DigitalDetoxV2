<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.digitaldetox.controller.ScreentimeController">

    <Button text="Refresh" fx:id="refresh" AnchorPane.topAnchor="10" AnchorPane.rightAnchor="10" onAction="#onRefreshButtonClick"/>
    <Button text="Back" fx:id="back" AnchorPane.topAnchor="10" AnchorPane.leftAnchor="10" onAction="#onBackButtonClick"/>

    <AnchorPane fx:id="chartPane" AnchorPane.topAnchor="50" AnchorPane.leftAnchor="10" AnchorPane.rightAnchor="10" AnchorPane.bottomAnchor="10">
    </AnchorPane>

</AnchorPane>
